<mvc:View 
	controllerName="ResearcherApp.RecycleFrontEndResearcherApp.controller.Overview"	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Page id="floatingFooterPage" showHeader="false" floatingFooter="false">
		<Text text="Overview" class="sapUiSmallMargin header"/>
		<HBox class="testtest">
			<!--mss proberen met een table?-->
			<List
			headerText="Time"
			items="{
				path: '/items'
			}" >
			<StandardListItem
				title="{time}"/>
			</List>
			<List
			headerText="Id"
			items="{
				path: '/items'
			}" >
			<StandardListItem
				title="{itemID}"/>
			</List>
			<List
			headerText="Status"
			items="{
				path: '/items'
			}" >
			<StandardListItem
				title="{status}"
				type="Navigation"/>
			</List>
		</HBox>
	</Page>
	<mvc:XMLView viewName="ResearcherApp.RecycleFrontEndResearcherApp.view.Footer"/>
</mvc:View>